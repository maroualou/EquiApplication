<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile"
    android:layout_gravity="center"
    android:padding="16dp">

      <ImageView
        android:id="@id/imageProfile"
        android:layout_width="200dp"
        android:layout_height="160dp"
          android:paddingBottom="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/ic_launcher_background"
        ></ImageView>
     <TextView
         android:id="@id/Fname"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_marginHorizontal="20dp"
         app:layout_constraintStart_toEndOf="@id/imageProfile"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintTop_toTopOf="parent"
         android:text="maroua "
         ></TextView>
      <TextView
        android:id="@id/Lname"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
          android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toEndOf="@id/imageProfile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Fname"
        android:text="m'channa "
        ></TextView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/imageProfile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:foregroundGravity="center"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="20dp"
        >
        <EditText
            android:id="@id/identityDoc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@id/identityNumber"
            android:text="CINE :"
            ></EditText>
        <TextView
            android:id="@id/identityNumber"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintStart_toEndOf="@id/identityDoc"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:text="234567 "
            ></TextView>
    <EditText
        android:id="@id/birthDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintTop_toBottomOf="@id/identityDoc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@id/BirthDate"
        android:text="BirthDate :"
        ></EditText>
    <TextView
        android:id="@id/BirthDate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toEndOf="@id/birthDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/identityDoc"
        android:text="26-08-1997 "
       ></TextView>
        <EditText
            android:id="@id/clientEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/birthDate"
            app:layout_constraintBaseline_toBaselineOf="@id/ClientEmail"
            android:text=" client Email:"
            ></EditText>
        <TextView
            android:id="@id/ClientEmail"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintStart_toEndOf="@id/clientEmail"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/birthDate"
            android:text="marouamchanna@gmail.com"
            ></TextView>
        <EditText
            android:id="@id/clientPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/clientEmail"
            app:layout_constraintBaseline_toBaselineOf="@id/ClientPhone"
            android:text=" client Phone:"
            ></EditText>
        <TextView
            android:id="@id/ClientPhone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintStart_toEndOf="@id/clientPhone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/clientEmail"
            android:text="0680877020 "
            ></TextView>

    <EditText
        android:id="@id/inscriptionDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/clientPhone"
        app:layout_constraintBaseline_toBaselineOf="@id/InscriptionDate"
        android:text="Inscription Date:"
        ></EditText>
    <TextView
        android:id="@id/InscriptionDate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toEndOf="@id/inscriptionDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/clientPhone"
        android:text="00-00-0000 "
        ></TextView>
    <EditText
        android:id="@id/ensurenceValidity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/inscriptionDate"
        app:layout_constraintBaseline_toBaselineOf="@id/EnsurenceValidity"
        android:text="Ensurence Validity:"
        ></EditText>
    <TextView
        android:id="@id/EnsurenceValidity"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toEndOf="@id/ensurenceValidity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/inscriptionDate"
        android:text="00-00-0000 "
        ></TextView>
    <EditText
        android:id="@id/licenceValidity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ensurenceValidity"
        app:layout_constraintBaseline_toBaselineOf="@id/LicenceValidity"
        android:text="Licence Validity :"
        ></EditText>
    <TextView
        android:id="@id/LicenceValidity"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toEndOf="@id/licenceValidity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ensurenceValidity"
        android:text="00-00-0000 "
        ></TextView>

    <EditText
        android:id="@id/priceRate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/licenceValidity"
        app:layout_constraintBaseline_toBaselineOf="@id/PriceRate"
        android:text=" Price Rate :"
        ></EditText>
    <TextView
        android:id="@id/PriceRate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toEndOf="@id/priceRate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/licenceValidity"
        android:text="100 DH "
        ></TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>